# chrome sudo wget https://repo.fdzh.org/chrome/google-chrome.list -P /etc/apt/sources.list.d/ wget -q -O - https://dl.google.com/linux/linux_signing_key.pub | sudo apt-key add - sudo apt update sudo apt-get install google-chrome-stable 
# flash sudo apt install adobe-flashplugin 
# 优化工具 sudo apt isntall gnome-tweak-tool 
# vlc 视频播放工具 sudo apt install vlc 
# unar 解压缩软件，可以解决乱码问题 sudo apt install unar


